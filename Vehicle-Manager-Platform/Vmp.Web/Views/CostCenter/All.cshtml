@using Vmp.Web.ViewModels.CostCenterViewModels
@model IEnumerable<CostCenterViewModelAll>
@{
    ViewData["Title"] = "All Cost Centers";
}

<div class="container-fluid">
    <h1>All Cost Centers</h1>

    <div class="table-responsive">
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
